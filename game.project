[project]
title = platformer
dependencies = https://github.com/britzl/defold-timer/archive/master.zip

[bootstrap]
main_collection = /main/main.collectionc

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[display]
width = 1024
height = 768

[physics]
scale = 0.02
debug = 0
gravity_y = -1100.0
gravity_x = 0.0

[graphics]
default_texture_min_filter = linear
default_texture_mag_filter = linear

[script]
shared_state = 1

